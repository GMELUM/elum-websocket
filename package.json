        "@types/pako": "^2.0.3",
        "@types/react": "^18.0.28",
        "@vitejs/plugin-react": "^4.3.0",
        "cross-env": "latest",
        "esbuild": "latest",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "nodemon": "latest",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "latest",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-dts": "latest",
        "rollup-plugin-esbuild": "latest",
        "rollup-plugin-web-worker-loader": "^1.6.1",
        "run-script-os": "^1.1.6",
        "solid-js": "^1.7.12",
        "ts-jest": "^29.1.1",
